{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "url": {
      "type": "string",
      "minLength": 1,
      "pattern": "(http[s]?://)?([^/s]+/)(.*)"
    },
    "orderBy": {
      "type": "string",
      "enum": ["key", "value"]
    }
  },
  "required": ["url"]
}
